{
  "name": "ado-task-viewer",
  "displayName": "Azure DevOps Task viewer",
  "description": "AzDo Task Viewer",
  "icon": "ado-task-viewer.png",
  "publisher": "carth",
  "version": "0.1.1",
  "private": "true",
  "repository": {
    "type": "git",
    "url": "https://github.com/c4rth/ado-task-viewer"
  },
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Azure",
    "Visualization"
  ],
  "keywords": [
    "task.json",
    "preview",
    "visualizer",
    "azure devops",
    "extension"
  ],
  "activationEvents": [
    "onCommand:ado-task-viewer.viewTask"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ado-task-viewer.viewTask",
        "title": "AzDo Task Viewer"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ado-task-viewer.viewTask",
          "group": "AzDoTaskViewerExtenstion",
          "when": "resourceFilename == task.json"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm-run-all vscepack:*",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "compile:view": "webpack --mode development",
    "vscepack:extension": "tsc -p ./",
    "vscepack:view": "webpack --mode production --devtool hidden-source-map",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:view": "webpack --watch --mode development",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src app --ext ts,tsx"
  },
  "devDependencies": {
    "@types/get-value": "^3.0.2",
    "@types/glob": "^7.1.4",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^17.0.11",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.3.0",
    "glob": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@fluentui/react": "^8.44.0",
    "@fluentui/react-hooks": "^8.3.7",
    "react": "^17.0.2",
    "react-collapse": "^5.1.1",
    "react-dom": "^17.0.2"
  }
}
