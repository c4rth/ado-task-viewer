{
  "name": "ado-task-viewer",
  "displayName": "ado-task-viewer",
  "description": "AzDo Task Viewer",
  "icon": "ado-task-viewer.png",
  "publisher": "carth",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/carthx/ado-task-viewer"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ado-task-viewer.adoTask"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ado-task-viewer.adoTask",
        "title": "AzDo Task Viewer"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ado-task-viewer.adoTask",
          "group": "AzDoTaskViewerExtenstion",
          "when": "resourceFilename == task.json"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm-run-all vscepack:*",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "compile:view": "webpack --mode development",
    "vscepack:extension": "tsc -p ./",
    "vscepack:view": "webpack --mode production --devtool hidden-source-map",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:view": "webpack --watch --mode development",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src app --ext ts,tsx"
  },
  "devDependencies": {
    "@types/get-value": "^3.0.2",
    "@types/glob": "^7.1.4",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^17.0.11",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.1.0",
    "glob": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@fluentui/react": "^8.41.0",
    "@fluentui/react-hooks": "^8.3.5",
    "react": "^17.0.2",
    "react-collapse": "^5.1.0",
    "react-dom": "^17.0.2"
  }
}